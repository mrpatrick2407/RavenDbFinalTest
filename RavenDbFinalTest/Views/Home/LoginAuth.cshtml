    <script>

        function birthdaynotification() {
            alert("S");
            var id = @(eid);
            var xhr = new XMLHttpRequest();
            var params = 'id=' + id.toString();
            xhr.open('GET', '/Admin/BirthdayNotification?' + params);
            xhr.setRequestHeader('Content-type', 'application/json');
            xhr.onload = function () {
                if (xhr.status === 200) {
                    alert("im 200");
                    const response = xhr.responseText;
                    alert(response);
                    if (response != "null") {
                        alert(1);
                        document.getElementById("wish").innerHTML = "Happy Birthday " + response + "!!!";
                        const confettiElement = document.querySelector('.confetti');
                        confettiElement.style.display = 'block';
                    } else {
                        alert(2);
                    }
                }
                else {
                    console.log('Error: ' + xhr.status);
                    alert("else");
                }
            }
            xhr.send();

        }
        window.onload = function () {
            birthdaynotification();
        };


    </script>

